{
  "name": "setup-GitHubActionsMSBuildLogger",
  "version": "1.0.0",
  "private": true,
  "description": "setup GitHubActionsMSBuildLogger action",
  "main": "lib/setup-GitHubActionsMSBuildLogger.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StanleyGoldman/setup-GitHubActionsMSBuildLogger.git"
  },
  "keywords": [
    "actions",
    "dotnet",
    "setup",
    "GitHubActionsMSBuildLogger"
  ],
  "author": "GitHub",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "fs-extra": "^8.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-circus": "^24.7.1",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "typescript": "^4.0.3"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "npm run build && npm run format",
      "post-commit": "npm prune --production && git add node_modules/* && git commit -m \"Husky commit correct node modules\""
    }
  }
}
